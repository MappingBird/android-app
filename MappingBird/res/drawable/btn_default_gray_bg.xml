<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape >
            <solid android:color="@color/graphic_btn_pressed"/>
            <stroke android:width="1.0dp"  
		        android:color="@color/graphic_btn_outline"  
		        android:dashWidth="0.0dp"  
		        android:dashGap="0.0dp" /> 
		    <corners android:radius="8px"/> 
        </shape> 
    </item>
    <item>
        <shape >
            <solid android:color="@color/graphic_btn_white"/>
            <stroke android:width="1.0dp"  
		        android:color="@color/graphic_btn_outline"  
		        android:dashWidth="0.0dp"  
		        android:dashGap="0.0dp" />
            <corners android:radius="8px"/>  
        </shape> 
    </item>

</selector>